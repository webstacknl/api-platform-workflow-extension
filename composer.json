{
  "name": "webstack/api-platform-workflow-extension",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "Opinionated integration of api platform with workflow component",
  "homepage": "https://github.com/webstacknl/api-platform-workflow-extension",
  "keywords": [ "workflow", "api-platform" ],
  "authors": [
    {
      "name": "Webstack B.V.",
      "email": "support@webstack.nl"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "api-platform/core": "^2.7",
    "doctrine/orm": "^2.7",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/serializer": "^6.4",
    "symfony/workflow": "^6.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.51",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3.59",
    "phpstan/phpstan-symfony": "^1.3.7"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Webstack\\ApiPlatformWorkflowBundle\\": "src/"
    }
  }
}
